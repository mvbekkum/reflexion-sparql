#+ endpoint: http://fuseki:3030/reflexion/query
#+ method: GET
#+ summary: Retrieves recommended settings for MRData.

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX refl: <http://reflexion/acr#>

SELECT ?data ?recommendedsetting
WHERE {
  ?data a refl:MrScan.
  ?data refl:hasRecommendedSetting ?recommendedsetting
}

